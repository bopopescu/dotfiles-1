{
    "window.zoomLevel": 0,
    "editor.rulers": [
        80
    ],
    "go.autocompleteUnimportedPackages": true,
    "go.lintOnSave": "package",
    "go.buildOnSave": "package",
    "go.vetOnSave": "package",
    "go.buildTags": "",
    "go.buildFlags": [],
    "go.lintTool": "golint",
    "go.lintFlags": [],
    "go.vetFlags": [],
    "go.coverOnSave": false,
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.formatTool": "goimports",
    "go.formatFlags": [],
    "go.goroot": "/usr/local/opt/go/libexec",
    "go.gopath": "/usr/local/go:/Users/zeeraw/go",
    "go.toolsGopath": "/usr/local/gotools",
    "go.gocodeFlags": [
        "-debug",
        "-builtin",
        "-ignore-case",
        "-unimported-packages"
    ],
    "go.gocodeAutoBuild": false,
    // "go.buildOnSave": "off", // for use with language server
    // "go.vetOnSave": "off", // for use with language server
    "[go]": { // for use with language server
        "editor.snippetSuggestions": "top",
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        },
        "editor.formatOnSave": true,
    },
    "go.useLanguageServer": false,
    "go.alternateTools": {
        "go-langserver": "gopls",
    },
    "go.languageServerFlags": [
    ],
    "go.languageServerExperimentalFeatures": {
        "format": true,
        "autoComplete": true,
        "diagnostics": true,
        "goToDefinition": true,
        "hover": true,
        "signatureHelp": true,
        "goToTypeDefinition": true,
        "documentSymbols": true,
        "rename": true,
        "findReferences": true,
        "workspaceSymbols": true,
    },
    "go.editorContextMenuCommands": {
        "toggleTestFile": false,
        "addTags": false,
        "removeTags": false,
        "testAtCursor": false,
        "testFile": false,
        "testPackage": false,
        "generateTestForFunction": false,
        "generateTestForFile": false,
        "generateTestForPackage": false,
        "addImport": true,
        "testCoverage": false,
        "playground": false
    },
    "files.associations": {
        "*.sql": "sql",
        "*.mysql": "sql",
        "*.postgresql": "postgres",
        "*.env.prod": "dotenv",
        ".env*": "dotenv",
        "*.postgres.sql": "postgres",
        "*.pg.sql": "postgres",
        "*.yml": "yaml",
    },
    "editor.multiCursorModifier": "ctrlCmd",
    "[yaml]": {
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": true
    },
}